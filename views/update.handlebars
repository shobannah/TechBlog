<form class="update-form m-5" data-id={{id}}>
    <div>
        <div class="card text-center m-5 border-0">
            <div class="card-body">
                <h5 class="headers m-3">Title:</h5>
                <textarea class="card-title text-center rounded w-75" rows="1" type="text" id="project-name">{{formName}}</textarea>
            </div>
            <ul class="text-center row">              
                <div class="col">
                    <h5 class="headers">Content:</h5>
                    <textarea class="form-control text-center rounded" rows="7" id="inputInstructionsTextArea">{{instructions}}</textarea>
                </div>
            </ul>
        </div>
    </div> 
    <div class="text-center">
        <button type="submit" class="btn btn-outline-success">Submit</button>
    <div>

</form>


<script src="/js/update.js"></script>
