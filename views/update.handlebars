<form class="update-form m-5" data-id={{id}}>
    <div>
        <div class="text-center">
            <div>
                <h5 class="headers m-3">Title:</h5>
                <textarea class="text-center rounded w-75" rows="1" type="text" name="project.title" id="project-name">{{formName}}</textarea>
            </div>              
            <div>
                <h5 class="headers m-3">Content:</h5>
                <textarea class="text-center rounded w-75" type="text" rows="10" name="project.description" id="inputInstructionsTextArea">{{instructions}}</textarea>
            </div>
        </div>
    </div> 
    <div class="text-center">
        <button type="submit" class="btn btn-outline-success">Submit</button>
    <div>

</form>


<script src="/js/update.js"></script>
